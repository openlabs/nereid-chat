<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data>
        <record id="chat_page" model="nereid.url_rule">
            <field name="rule">/nereid-chat/chat.js</field>
            <field name="endpoint">nereid.chat.chat_js</field>
            <field name="sequence" eval="60" />
            <field name="http_method_get" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="chat_page_base" model="nereid.url_rule">
            <field name="rule">/nereid-chat/chat-base</field>
            <field name="endpoint">nereid.chat.chat_template</field>
            <field name="sequence" eval="60" />
            <field name="http_method_get" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="chat_stream" model="nereid.url_rule">
            <field name="rule">/nereid-chat/stream</field>
            <field name="endpoint">nereid.chat.stream</field>
            <field name="sequence" eval="60" />
            <field name="http_method_get" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="chat_send_message" model="nereid.url_rule">
            <field name="rule">/nereid-chat/send-message</field>
            <field name="endpoint">nereid.chat.send_message</field>
            <field name="sequence" eval="60" />
            <field name="http_method_post" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="chat_start_session" model="nereid.url_rule">
            <field name="rule">/nereid-chat/start-session</field>
            <field name="endpoint">nereid.chat.start_session</field>
            <field name="sequence" eval="60" />
            <field name="http_method_post" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
        <record id="chat_get_friends" model="nereid.url_rule">
            <field name="rule">/nereid-chat/get-friends</field>
            <field name="endpoint">nereid.user.chat_friends</field>
            <field name="sequence" eval="60" />
            <field name="http_method_get" eval="True" />
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
    </data>
</tryton>
